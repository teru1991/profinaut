{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "contracts/schemas/marketdata/md_orderbook_gap.schema.json",
  "title": "md_orderbook_gap",
  "type": "object",
  "required": ["raw_msg_id", "venue_id", "market_id", "received_ts"],
  "properties": {
    "raw_msg_id": {"type": "string"},
    "venue_id": {"type": "string"},
    "market_id": {"type": "string"},
    "received_ts": {"type": "string", "format": "date-time"},
    "event_ts": {"type": ["string", "null"], "format": "date-time"},
    "seq": {"type": ["integer", "null"]},
    "prev_seq": {"type": ["integer", "null"]},
    "next_seq": {"type": ["integer", "null"]},
    "levels": {
      "type": "object",
      "properties": {
        "bids": {"type": "array"},
        "asks": {"type": "array"}
      }
    },
    "extra_json": {"type": "object"}
  }
}
