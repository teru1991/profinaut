# Shared
COMPOSE_PROJECT_NAME=profinaut-dataplat

# PostgreSQL
POSTGRES_DB=profinaut
POSTGRES_USER=profinaut_app
POSTGRES_PASSWORD=change-me-postgres
POSTGRES_PORT=5432
POSTGRES_MEM_LIMIT=768m
POSTGRES_CPUS=1.0

# ClickHouse
CLICKHOUSE_DB=profinaut
CLICKHOUSE_USER=profinaut_app
CLICKHOUSE_PASSWORD=change-me-clickhouse
CLICKHOUSE_HTTP_PORT=8123
CLICKHOUSE_NATIVE_PORT=9000
CLICKHOUSE_MEM_LIMIT=2g
CLICKHOUSE_CPUS=2.0

# Valkey
VALKEY_PASSWORD=change-me-valkey
VALKEY_PORT=6379
VALKEY_MEM_LIMIT=512m
VALKEY_CPUS=0.75

# Object store (SeaweedFS S3)
OBJECTSTORE_ACCESS_KEY=change-me-objectstore-access
OBJECTSTORE_SECRET_KEY=change-me-objectstore-secret
OBJECTSTORE_REGION=us-east-1
OBJECTSTORE_BUCKETS=profinaut-bronze,profinaut-silver,profinaut-gold
SEAWEEDFS_MASTER_PORT=9333
SEAWEEDFS_FILER_PORT=8888
SEAWEEDFS_S3_PORT=8333
OBJECTSTORE_MEM_LIMIT=1024m
OBJECTSTORE_CPUS=1.5
